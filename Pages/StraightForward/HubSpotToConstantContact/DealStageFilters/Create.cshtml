@page
@model AudiocraticAPI.Pages.StraightForward.HubSpotToConstantContact.DealStageFilters.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h2>Create</h2>

<h4>DealStageFilter</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label class="control-label"></label>
                <select id="DealStages" name="DealStages" class ="form-control" asp-items="ViewBag.UserId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Add" class="btn btn-success" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script>
        $(function(){
            $("#DealStages").chosen();
        });    
    </script>
}
